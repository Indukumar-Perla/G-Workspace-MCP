{
  "name": "google-calendar-mcp",
  "version": "1.0.0",
  "description": "Google Calendar MCP Server",
  "type": "module",
  "bin": {
    "google-calendar-mcp": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node build/index.js",
    "auth": "node build/auth-server.js",
    "dev": "ts-node --esm src/index.ts",
    "postinstall": "npm run build && echo '#!/usr/bin/env node\n' | cat - build/index.js > temp && mv temp build/index.js && chmod +x build/index.js"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@modelcontextprotocol/sdk": "^1.0.3",
    "@types/express": "^4.17.21",
    "express": "^4.18.2",
    "force": "^0.0.3",
    "google-auth-library": "^9.15.0",
    "googleapis": "^144.0.0",
    "install": "^0.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "typescript": "^5.3.3"
  }
}
